    <div id="Inavtivity" style="display:none; cursor: default">
        <h1>You will be logged off soon due to inactivity.</h1>
        <div class="two-button-wrapper text-center pagination-btn" style="margin-bottom:30px; margin-top:0px;">
            <a style="margin-top:15px" class="btn primary-btn" id="mainok">Ok</a>
        </div>
    </div>

  
    <script type="text/javascript">
        var sessionMinutes = 30.5;
        $('#mainok').click(function () {
            sessionMinutes = sessionMinutes + sessionMinutes;
            $.unblockUI();
            return false;
        });
        $(document).ready(function () {           
            function ilSessionTimeout() {
                var hostweburl = decodeURIComponent(getQueryStringParameter("SPHostUrl"));
                window.location.href = '/Home/Index?SPHostUrl=' + hostweburl;
            };
            function ilSessionTimeoutWarning() {
                $.blockUI({
                    message: $('#Inavtivity'), css: { width: '500px', margin: '-100px 0px 0px -100px' }
                });
            };
            var g_pageTimer = window.setTimeout(ilSessionTimeout, sessionMinutes * 60 * 1000);
            var g_pageTimerWarning = window.setTimeout(ilSessionTimeoutWarning, Math.round(sessionMinutes * 60 * 1000 * 0.8));

            window.onbeforeunload = function (e) {
                window.clearTimeout(g_pageTimer);
                window.clearTimeout(g_pageTimerWarning);
            };
        });
    </script>


